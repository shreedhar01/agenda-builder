{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node -r dotenv/config dist/index.js",
    "dev": "tsx watch -r dotenv/config src/index.ts",
    "lint": "eslint",
    "db:generate": "dotenv -e ./.env -- pnpm --filter @repo/database generate",
    "db:push": "dotenv -e ./.env -- pnpm --filter @repo/database push",
    "db:migrate": "pnpm run db:generate && pnpm run db:push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.24.0",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/dotenv": "^8.2.3",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.19.25",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.39.1",
    "tsx": "^4.21.0"
  },
  "dependencies": {
    "@repo/database": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/shared-types": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "bcrypt": "^6.0.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.0.1"
  }
}